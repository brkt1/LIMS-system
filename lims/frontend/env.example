# LIMS Frontend Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=30000

# Default Values
REACT_APP_DEFAULT_TENANT_ID=1
REACT_APP_DEFAULT_USER_ID=1
REACT_APP_DEFAULT_USER_EMAIL=admin@lims.com

# Security
REACT_APP_CLIENT_IP=unknown
REACT_APP_PASSWORD_MIN_LENGTH=8
REACT_APP_PASSWORD_MAX_LENGTH=128

# Application Settings
REACT_APP_APP_NAME=LIMS System
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=Laboratory Information Management System

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false

# Development Settings
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

# Support Configuration
REACT_APP_SUPPORT_PHONE=(555) 123-4567
REACT_APP_EMERGENCY_PHONE=(555) 911-HELP
REACT_APP_SUPPORT_EMAIL=support@clinic.com
REACT_APP_SUPPORT_HOURS=Monday-Friday, 8:00 AM - 6:00 PM
REACT_APP_CHAT_HOURS=Monday-Friday, 9:00 AM - 5:00 PM

# Billing Plans Configuration
REACT_APP_BASIC_PLAN_PRICE=$29/month
REACT_APP_BASIC_PLAN_MAX_USERS=10
REACT_APP_BASIC_PLAN_FEATURES=Basic Dashboard,Standard Support

REACT_APP_PRO_PLAN_PRICE=$79/month
REACT_APP_PRO_PLAN_MAX_USERS=50
REACT_APP_PRO_PLAN_FEATURES=Advanced Analytics,Priority Support,API Access

REACT_APP_ENTERPRISE_PLAN_PRICE=$199/month
REACT_APP_ENTERPRISE_PLAN_MAX_USERS=200
REACT_APP_ENTERPRISE_PLAN_FEATURES=All Features,Custom Branding,Dedicated Support

